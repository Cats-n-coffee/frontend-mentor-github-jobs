cription: '', location: '', full_time: false });\n    const { jobs, isPending, error } = useApiCall(filters);\n\n    const addFilters = (filter) => {\n        setFilters({...filters, description: filter.description, location: filter.location, full_time: filter.full_time})\n    }\n\n    \n\n    return (\n        <DesktopWrapper>\n            <MainWrapper>\n                    <Filters addFilters={ addFilters }/>\n                    { isPending ? <Spinner/> \n                    : <div>\n                    <ResultsStyled>\n                        {jobs.map(job => {\n                            return (\n                                <JobPosting \n                                    key={ job.id } \n                                    job={ job } \n                                    isSelected={ isSelected }\n                                    selectJob={ selectJob } />\n                            )\n                        })}\n                    </ResultsStyled>\n                    <ButtonLoadMore>Load More</ButtonLoadMore> \n                    </div> }\n            </MainWrapper>\n        </DesktopWrapper>\n    )\n}\n\nexport default Results;"},"lineCount":null}},"error":null,"hash":"593f5369a7d1e0dc35477fe3b87aef1c","cacheData":{"env":{}}}