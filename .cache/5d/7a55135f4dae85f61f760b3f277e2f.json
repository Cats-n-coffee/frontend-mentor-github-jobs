[key] === true) {\n                filters[key] = 'on';\n            }\n\n            return encodeURI(key) + '=' + encodeURI(filters[key])})\n        .join('&');\n\n    params = `?${params}`;\n      \n    urlTemplate = `${urlTemplate}${params}&page=${page}` ;\n\n\n    console.log(urlTemplate, 'page', page)\n    \n    useEffect(() => {\n        fetch(urlTemplate)\n            .then(res => {\n                if (!res.ok) {\n                    throw Error('Could not find any job matching your criteria');\n                }\n                return res.json()\n            })\n            .then(data => {\n                console.log(data);\n                setJobs(data)\n                setIsPending(false)\n                setError(null)\n            })\n            .catch(err => {\n                setIsPending(false)\n                setError(err.message)\n            })\n        \n    }, [description, location, full_time, page]);\n\n    return { jobs, isPending, error }\n}\n\nexport default useApiCall;"},"lineCount":null}},"error":null,"hash":"f4f9df45b27efe06d1db585f7f844c09","cacheData":{"env":{}}}